interface Tree {
    void fruits(String... fruitNames);
    void leaves(int... leafCounts);
    void flowers(int... flowerCounts);
}

class Branch1 implements Tree {
    public void fruits(String... fruitNames) {
        System.out.println("Branch1 Fruits: " + Arrays.toString(fruitNames));
    }

    public void leaves(int... leafCounts) {
        System.out.println("Branch1 Leaves: " + Arrays.toString(leafCounts));
    }

    public void flowers(int... flowerCounts) {
        System.out.println("Branch1 Flowers: " + Arrays.toString(flowerCounts));
    }
}

class Branch2 implements Tree {
    public void fruits(String... fruitNames) {
        System.out.println("Branch2 Fruits: " + Arrays.toString(fruitNames));
    }

    public void leaves(int... leafCounts) {
        System.out.println("Branch2 Leaves: " + Arrays.toString(leafCounts));
    }

    public void flowers(int... flowerCounts) {
        System.out.println("Branch2 Flowers: " + Arrays.toString(flowerCounts));
    }
}
