class Vehicle {
    int speed;
    String fuelType;

    public Vehicle(int speed, String fuelType) {
        this.speed = speed;
        this.fuelType = fuelType;
    }
}

class Car extends Vehicle {
    public Car(int speed, String fuelType) {
        super(speed, fuelType);
    }
}

class ElectricCar extends Car {
    int batteryCapacity;

    public ElectricCar(int speed, String fuelType, int batteryCapacity) {
        super(speed, fuelType);
        this.batteryCapacity = batteryCapacity;
    }

    public void display() {
        System.out.println("Speed: " + speed + ", Fuel Type: " + fuelType + ", Battery Capacity: " + batteryCapacity);
    }
}

public class Main {
    public static void main(String[] args) {
        ElectricCar electricCar = new ElectricCar(120, "Electric", 100);
        electricCar.display();
    }
}
